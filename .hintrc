{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "axe/parsing": "off",
    "no-inline-styles": "off",
    "disown-opener": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18",
    "not safari <= 18",
    "not and_ff <= 130",
    "not firefox <= 130"
  ]
}